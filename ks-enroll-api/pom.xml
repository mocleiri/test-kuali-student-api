    <build>
        <resources>
            <resource>
                <directory>${project.basedir}/src/main/resources</directory>
            </resource>
            <resource>
                <directory>${project.build.directory}/generated-sources/datadictionary</directory>
            </resource>
        </resources>
        <plugins>
            <plugin>
                <groupId>org.kuali.maven.plugins</groupId>
                <artifactId>maven-kscontractdoc-plugin</artifactId>
                <version>${ks-contract-docs.plugin.version}</version>
                <!-- For instructions see https://wiki.kuali.org/display/STUDENT/Understanding+the+Contract+Doc+Maven+Plugin  -->
                <executions>
                    <!-- This generates the standard contract docs on the site-->
                    <execution>
                        <id>generate-contract-documentation</id>
                        <phase>${ks.contractdoc.phase}</phase>
                        <goals>
                            <goal>kscontractdoc</goal>
                        </goals>
                    </execution>
                    <!-- This generates the the dictionary documentation docs on the site-->
                    <execution>
                        <id>generate-dictionary-documentation</id>
                        <phase>${ks.dictionarydoc.phase}</phase>
                        <configuration>
                            <inputFiles>
                                <inputFile>ks-TypeInfo-dictionary.xml</inputFile>
                            </inputFiles>
                        </configuration>
                        <goals>
                            <goal>ksdictionarydoc</goal>
                        </goals>

                    </execution>
                    <!-- This generates the the ks-xxxx-generated.xml files in target/classes -->
                    <execution>
                        <id>generate-dictionary-files</id>
                        <phase>${ks.dictionarycreate.phase}</phase>
                        <goals>
                            <goal>ksdictionarycreator</goal>
                        </goals>
                        <configuration>
                            <writeManual>true</writeManual>
                            <classNames>
                                <!--                                 Atp -->
                                <className>AtpInfo</className>
                                <className>MilestoneInfo</className>
                                <className>AtpAtpRelationInfo</className>
                                <!--                                 Acal -->
                                <className>AcademicCalendarInfo</className>
                                <className>AcalEventInfo</className>
                                <className>HolidayCalendarInfo</className>
                                <className>HolidayInfo</className>
                                <className>TermInfo</className>
                                <className>KeyDateInfo</className>
                                <!--                             Appointment -->
                                <className>AppointmentWindowInfo</className>
                                <className>AppointmentSlotInfo</className>
                                <className>AppointmentInfo</className>
                                <!--                                LPR -->
                                <className>LuiPersonRelationInfo</className>
                                <className>LprRosterInfo</className>
                                <className>LprRosterEntryInfo</className>
                                <className>LprTransactionInfo</className>
                                <className>LprTransactionItemInfo</className>
                                <className>RequestOptionInfo</className>
                                <className>LprTransactionItemResultInfo</className>
                                <!--                                Hold         -->
                                <className>HoldInfo</className>
                                <className>IssueInfo</className>
                                <!--                                LUI                 -->
                                <className>LuiInfo</className>
                                <className>LuiIdentifierInfo</className>
                                <className>LuCodeInfo</className>
                                <className>FeeInfo</className>
                                <className>CurrencyAmountInfo</className>
                                <className>RevenueInfo</className>
                                <className>AffiliatedOrgInfo</className>
                                <className>MeetingScheduleInfo</className>
                                <className>LuiLuiRelationInfo</className>
                                <className>LuiCapacityInfo</className>
                                <!--                                Course Offering      -->
                                <className>CourseOfferingInfo</className>
                                <className>CourseWaitlistEntryInfo</className>
                                <className>OfferingInstructorInfo</className>
                                <className>FormatOfferingInfo</className>
                                <className>ActivityOfferingInfo</className>
                                <className>RegistrationGroupInfo</className>
                                <className>SeatPoolDefinitionInfo</className>
                                <!-- Course registration -->
                                <className>CourseRegistrationInfo</className>
                                <className>ActivityRegistrationInfo</className>
                                <className>RegGroupRegistrationInfo</className>
                                <className>RegRequestInfo</className>
                                <className>RegRequestItemInfo</className>
                                <className>RegResponseInfo</className>
                                <className>RegResponseItemInfo</className>
                                <!-- Exemption -->
                                <className>ExemptionInfo</className>
                                <className>ExemptionRequestInfo</className>
                                <!-- Grading -->
                                <className>GradeRosterInfo</className>
                                <className>GradeRosterEntryInfo</className>
                                <className>GradeValuesGroupInfo</className>
                                <!-- LRR -->
                                <className>LearningResultRecordInfo</className>
                                <className>ResultSourceInfo</className>
                                <!-- LRC -->
                                <className>ResultValuesGroupInfo</className>
                                <className>ResultValueInfo</className>
                                <className>ResultScaleInfo</className>
                                <!-- Course -->
                                <className>CourseInfo</className>
                                <className>FormatInfo</className>
                                <className>ActivityInfo</className>
                                <className>LoDisplayInfo</className>
                                <!-- Program -->
                                <className>CoreProgramInfo</className>
                                <className>CredentialProgramInfo</className>
                                <className>HonorsProgramInfo</className>
                                <className>MajorDisciplineInfo</className>
                                <className>ProgramRequirementInfo</className>
                                <className>ProgramVariationInfo</className>
                                <!-- Type -->
                                <className>TypeInfo</className>
                                <className>TypeTypeRelationInfo</className>
                                <!-- State -->
                                <className>StateInfo</className>
                                <className>LifecycleInfo</className>
                            </classNames>
                        </configuration>
                    </execution>
                </executions>
                <configuration>
                    <!-- this defines that location of the source code used for all contract docs executions -->
                    <sourceDirs>
                        <sourceDir>${project.basedir}/src/main/java</sourceDir>
                        <!-- core api -->
                        <sourceDir>${project.basedir}/../ks-core-api/src/main/java</sourceDir>
                        <!-- common api -->
