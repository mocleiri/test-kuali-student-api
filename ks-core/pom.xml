  <reporting>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>findbugs-maven-plugin</artifactId>
        <version>${findbugs.plugin.version}</version>
        <configuration>
          <findbugsXmlOutput>true</findbugsXmlOutput>
          <!-- Optional directory to put findbugs xml report -->
          <findbugsXmlOutputDirectory>target/site</findbugsXmlOutputDirectory>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-project-info-reports-plugin</artifactId>
        <configuration>
          <!-- this takes forever to complete, enable as needed -->
          <!--
                    <dependencyLocationsEnabled>false</dependencyLocationsEnabled>
                    -->
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>${javadoc.plugin.version}</version>
        <configuration>
          <maxmemory>256m</maxmemory>
          <aggregate>true</aggregate>
          <show>private</show>
          <bottom>Copyright © 2009 The Kuali Foundation. All rights reserved.</bottom>
          <excludePackageNames>*.service.jaxws:*.exceptions.jaxws</excludePackageNames>
          <source>1.6</source>
          <links>
            <link>http://cxf.apache.org/javadoc/${cxf.version}/</link>
            <link>http://java.sun.com/javase/6/docs/api/</link>
            <link>http://java.sun.com/javaee/5/docs/api/</link>
            <link>http://static.springframework.org/spring/docs/${spring.version}/api/</link>
            <link>http://static.springsource.org/spring-security/site/docs/${spring.security.javadoc.version}/apidocs/</link>
            <link>http://student.kuali.org/site/security/${ks-standard-security.version}/apidocs/</link>
            <link>http://rice.kuali.org/docs/${kuali.rice.version}/site/apidocs/</link>
            <link>http://logging.apache.org/log4j/${log4j.javadoc.version}/apidocs/</link>
            <link>http://ws.apache.org/wss4j/apidocs/</link>
          </links>
        </configuration>
        <reportSets>
          <reportSet>
            <reports>
              <report>javadoc</report>
            </reports>
          </reportSet>
        </reportSets>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>cobertura-maven-plugin</artifactId>
        <version>${cobertura.plugin.version}</version>
        <configuration>
          <formats>
            <format>html</format>
            <!--
                        <format>xml</format>
                        -->
          </formats>
        </configuration>
      </plugin>
    </plugins>
  </reporting>
