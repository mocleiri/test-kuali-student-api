<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
 <import resource="classpath:ks-base-dictionary-context.xml"/>

 <!-- AtpInfo-->
 <bean id="org.kuali.student.core.atp.dto.AtpInfo-parent" abstract="true" parent="objectStructureDefinition">
  <property name="name" value="org.kuali.student.core.atp.dto.AtpInfo"/>
  <property name="attributes">
   <list>
    <ref bean="atpInfo.desc"/>
    <ref bean="atpInfo.endDate"/>
    <ref bean="atpInfo.id"/>
    <ref bean="atpInfo.name"/>
    <ref bean="atpInfo.startDate"/>
    <ref bean="atpInfo.state"/>
    <ref bean="atpInfo.type"/>
   </list>
  </property>
  <property name="hasMetaData" value="true"/>
 </bean>
 <bean id="org.kuali.student.core.atp.dto.AtpInfo" parent="org.kuali.student.core.atp.dto.AtpInfo-parent"/>

 <bean id="atpInfo.desc-parent" abstract="true" parent="baseComplex">
  <property name="name" value="desc"/>
  <property name="dataObjectStructure" ref="org.kuali.student.common.dto.RichTextInfo"/>
 </bean>
 <bean id="atpInfo.desc" parent="atpInfo.desc-parent"/>

 <bean id="atpInfo.endDate-parent" abstract="true" parent="baseDate">
  <property name="name" value="endDate"/>
 </bean>
 <bean id="atpInfo.endDate" parent="atpInfo.endDate-parent"/>

 <bean id="atpInfo.id-parent" abstract="true" parent="baseKualiTypeRequired">
  <property name="name" value="id"/>
 </bean>
 <bean id="atpInfo.id" parent="atpInfo.id-parent"/>

 <bean id="atpInfo.name-parent" abstract="true" parent="baseKualiName">
  <property name="name" value="name"/>
 </bean>
 <bean id="atpInfo.name" parent="atpInfo.name-parent"/>

 <bean id="atpInfo.startDate-parent" abstract="true" parent="baseDate">
  <property name="name" value="startDate"/>
 </bean>
 <bean id="atpInfo.startDate" parent="atpInfo.startDate-parent"/>

 <bean id="atpInfo.state-parent" abstract="true" parent="baseKualiStateRequired">
  <property name="name" value="state"/>
 </bean>
 <bean id="atpInfo.state" parent="atpInfo.state-parent"/>

 <bean id="atpInfo.type-parent" abstract="true" parent="baseKualiTypeRequired">
  <property name="name" value="type"/>
 </bean>
 <bean id="atpInfo.type" parent="atpInfo.type-parent"/>

