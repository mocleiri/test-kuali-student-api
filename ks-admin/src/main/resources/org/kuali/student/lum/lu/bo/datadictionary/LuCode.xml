<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="LuCode" parent="LuCode-parentBean" />
  <bean id="LuCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.student.lum.lu.bo.LuCode" />
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="LU Code" />
    <property name="lookupDefinition">
      <ref bean="LuCode-lookupDefinition" />
    </property>
    <property name="inquiryDefinition">
      <ref bean="LuCode-inquiryDefinition" />
    </property>
    <property name="attributes">
      <list>
        <ref bean="LuCode-id" />
        <ref bean="LuCode-createId" />
        <ref bean="LuCode-createDate" />
        <ref bean="LuCode-updateId" />
        <ref bean="LuCode-updateDate" />
        <ref bean="LuCode-description" />
        <ref bean="LuCode-type" />
        <ref bean="LuCode-value" />
        <ref bean="LuCode-cluId" />       
      </list>
    </property>
  </bean>

  <alias alias="LuCode-id" name="CoreAttributeReference-id" />  
  <alias alias="LuCode-createId" name="CoreAttributeReference-createId" />
  <alias alias="LuCode-createDate" name="CoreAttributeReference-createDate" />
  <alias alias="LuCode-updateId" name="CoreAttributeReference-updateId" />
  <alias alias="LuCode-updateDate" name="CoreAttributeReference-updateDate" />
  
  <alias alias="LuCode-description" name="CoreAttributeReference-description" />
  
  <bean id="LuCode-type" parent="LuCode-type-parentBean"/>
  <bean id="LuCode-type-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
    <property name="name" value="type"/>
    <property name="shortLabel" value="Type"/>
    <property name="summary" value="Type"/>
    <property name="label" value="Type"/>
    <property name="description" value="Type"/>
  </bean>
  
  <bean id="LuCode-value" parent="LuCode-value-parentBean"/>
  <bean id="LuCode-value-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
    <property name="name" value="value"/>
    <property name="shortLabel" value="Value"/>
    <property name="summary" value="Value"/>
    <property name="label" value="Value"/>
    <property name="description" value="Value"/>
  </bean>
  
  <bean id="LuCode-cluId" parent="LuCode-cluId-parentBean"/>
  <bean id="LuCode-cluId-parentBean" abstract="true" parent="CoreAttributeReference-id">
    <property name="name" value="cluId"/>
    <property name="shortLabel" value="CLU ID"/>
    <property name="summary" value="CLU ID"/>
    <property name="label" value="CLU ID"/>
    <property name="description" value="CLU ID"/>
  </bean>
    

  <bean id="LuCode-lookupDefinition" parent="LuCode-lookupDefinition-parentBean" />
  <bean id="LuCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="LU Code Lookup" />
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="type" />
        <bean parent="FieldDefinition" p:attributeName="description" />
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="type" />
        <bean parent="FieldDefinition" p:attributeName="value" />
        <bean parent="FieldDefinition" p:attributeName="description" />
        <bean parent="FieldDefinition" p:attributeName="cluId" />
      </list>
    </property>
  </bean>

  <bean id="LuCode-inquiryDefinition" parent="LuCode-inquiryDefinition-parentBean" />
  <bean id="LuCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="LU Code Inquiry" />
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Overview" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="id" />
              <bean parent="FieldDefinition" p:attributeName="type" />
              <bean parent="FieldDefinition" p:attributeName="value" />
              <bean parent="FieldDefinition" p:attributeName="description" />
              <bean parent="FieldDefinition" p:attributeName="cluId" />
            </list>
          </property>
        </bean>
        <ref bean="LuCode-inquirySection-metaInformation" />
      </list>
    </property>
  </bean>
  
  <alias alias="LuCode-inquirySection-metaInformation" name="CoreAttributeReference-inquirySection-metaInformation" />

</beans>