<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="CluIdentifier" parent="CluIdentifier-parentBean" />
    <bean id="CluIdentifier-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.student.lum.lu.bo.CluIdentifier" />
        <property name="titleAttribute" value="id" />
        <property name="objectLabel" value="CLU Identifier" />
        <property name="lookupDefinition">
            <ref bean="CluIdentifier-lookupDefinition" />
        </property>
        <property name="inquiryDefinition">
            <ref bean="CluIdentifier-inquiryDefinition" />
        </property>
        <property name="attributes">
            <list>
                <ref bean="CluIdentifier-id" />
                <ref bean="CluIdentifier-code" />
                <ref bean="CluIdentifier-shortName" />
                <ref bean="CluIdentifier-longName" />
                <ref bean="CluIdentifier-level" />
                <ref bean="CluIdentifier-division" />
                <ref bean="CluIdentifier-variation" />
                <ref bean="CluIdentifier-suffixCode" />
                <ref bean="CluIdentifier-orgId" />
                <ref bean="CluIdentifier-type" />
                <ref bean="CluIdentifier-state" />
            </list>
        </property>
    </bean>

    <alias alias="CluIdentifier-id" name="CoreAttributeReference-id" />

    <bean id="CluIdentifier-code" parent="CluIdentifier-code-parentBean" />
    <bean id="CluIdentifier-code-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
      <property name="shortLabel" value="Code"/>
      <property name="summary" value="Code for the identifier"/>
      <property name="name" value="code"/>
      <property name="label" value="Code"/>
      <property name="description" value="Code for the identifier"/>
    </bean>

    <bean id="CluIdentifier-shortName" parent="CluIdentifier-shortName-parentBean" />
    <bean id="CluIdentifier-shortName-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
      <property name="shortLabel" value="Short Name"/>
      <property name="summary" value="Short name for the identifier"/>
      <property name="name" value="shortName"/>
      <property name="label" value="Short Name"/>
      <property name="description" value="Short name for the identifier"/>
    </bean>

    <bean id="CluIdentifier-longName" parent="CluIdentifier-longName-parentBean" />
    <bean id="CluIdentifier-longName-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
      <property name="shortLabel" value="Long Name"/>
      <property name="summary" value="Long name for the identifier"/>
      <property name="name" value="longName"/>
      <property name="label" value="Long Name"/>
      <property name="description" value="Long name for the identifier"/>
    </bean>
    
    <bean id="CluIdentifier-level" parent="CluIdentifier-level-parentBean" />
    <bean id="CluIdentifier-level-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
      <property name="shortLabel" value="Level"/>
      <property name="summary" value="Level for the identifier"/>
      <property name="name" value="level"/>
      <property name="label" value="Level"/>
      <property name="description" value="Level for the identifier"/>
    </bean>
    
    <bean id="CluIdentifier-division" parent="CluIdentifier-division-parentBean" />
    <bean id="CluIdentifier-division-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
      <property name="shortLabel" value="Division"/>
      <property name="summary" value="Division for the identifier"/>
      <property name="name" value="division"/>
      <property name="label" value="Division"/>
      <property name="description" value="Division for the identifier"/>
    </bean>
    
    <bean id="CluIdentifier-variation" parent="CluIdentifier-variation-parentBean" />
    <bean id="CluIdentifier-variation-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
      <property name="shortLabel" value="Variation"/>
      <property name="summary" value="Variation for the identifier"/>
      <property name="name" value="variation"/>
      <property name="label" value="Variation"/>
      <property name="description" value="Variation for the identifier"/>
    </bean>
    
    <bean id="CluIdentifier-suffixCode" parent="CluIdentifier-suffixCode-parentBean" />
    <bean id="CluIdentifier-suffixCode-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
      <property name="shortLabel" value="Suffix Code"/>
      <property name="summary" value="Suffix Code for the identifier"/>
      <property name="name" value="suffixCode"/>
      <property name="label" value="Suffix Code"/>
      <property name="description" value="Suffix code for the identifier"/>
    </bean>
    
    <alias alias="CluIdentifier-orgId" name="CoreAttributeReference-orgId" />
    
    <bean id="CluIdentifier-type" parent="CluIdentifier-type-parentBean" />
    <bean id="CluIdentifier-type-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
      <property name="shortLabel" value="Type"/>
      <property name="summary" value="Type for the identifier"/>
      <property name="name" value="type"/>
      <property name="label" value="Type"/>
      <property name="description" value="Type for the identifier"/>
    </bean>
    
    <bean id="CluIdentifier-state" parent="CluIdentifier-state-parentBean" />
    <bean id="CluIdentifier-state-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
      <property name="shortLabel" value="State"/>
      <property name="summary" value="State"/>
      <property name="name" value="state"/>
      <property name="label" value="State"/>
      <property name="description" value="State"/>
    </bean>


    <bean id="CluIdentifier-lookupDefinition" parent="CluIdentifier-lookupDefinition-parentBean" />
    <bean id="CluIdentifier-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
      <property name="title" value="CLU Identifier Lookup" />
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="longName" />
                <bean parent="FieldDefinition" p:attributeName="code" />
                <bean parent="FieldDefinition" p:attributeName="level" />
                <bean parent="FieldDefinition" p:attributeName="division" />
            </list>
        </property>
        <property name="resultFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="id" />
              <bean parent="FieldDefinition" p:attributeName="code" />
              <bean parent="FieldDefinition" p:attributeName="shortName" />
              <bean parent="FieldDefinition" p:attributeName="longName" />
              <bean parent="FieldDefinition" p:attributeName="level" />
              <bean parent="FieldDefinition" p:attributeName="division" />
              <bean parent="FieldDefinition" p:attributeName="state" />
            </list>
        </property>
    </bean>

    <bean id="CluIdentifier-inquiryDefinition" parent="CluIdentifier-inquiryDefinition-parentBean" />
    <bean id="CluIdentifier-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="CLU Identifier Inquiry" />
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="CLU Identifier" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                          <bean parent="FieldDefinition" p:attributeName="id" />
                          <bean parent="FieldDefinition" p:attributeName="code" />
                          <bean parent="FieldDefinition" p:attributeName="shortName" />
                          <bean parent="FieldDefinition" p:attributeName="longName" />
                          <bean parent="FieldDefinition" p:attributeName="level" />
                          <bean parent="FieldDefinition" p:attributeName="division" />
                          <bean parent="FieldDefinition" p:attributeName="variation" />
                          <bean parent="FieldDefinition" p:attributeName="suffixCode" />
                          <bean parent="FieldDefinition" p:attributeName="type" />
                          <bean parent="FieldDefinition" p:attributeName="state" />
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

</beans>