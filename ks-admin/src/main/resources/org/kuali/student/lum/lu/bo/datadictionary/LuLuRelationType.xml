<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="LuLuRelationType" parent="LuLuRelationType-parentBean" />
  <bean id="LuLuRelationType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.student.lum.lu.bo.LuLuRelationType" />
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="LU Relation Type" />
    <property name="lookupDefinition">
      <ref bean="LuLuRelationType-lookupDefinition" />
    </property>
    <property name="inquiryDefinition">
      <ref bean="LuLuRelationType-inquiryDefinition" />
    </property>
    <property name="attributes">
      <list>
        <ref bean="LuLuRelationType-id" />
        <ref bean="LuLuRelationType-createId" />
        <ref bean="LuLuRelationType-createDate" />
        <ref bean="LuLuRelationType-updateId" />
        <ref bean="LuLuRelationType-updateDate" />
        <ref bean="LuLuRelationType-activeFromDate" />
        <ref bean="LuLuRelationType-activeToDate" />
        <ref bean="LuLuRelationType-active" />        
        <ref bean="LuLuRelationType-description" />
        <ref bean="LuLuRelationType-name" />
        <ref bean="LuLuRelationType-reverseDescription" />
        <ref bean="LuLuRelationType-reverseName" />
      </list>
    </property>
  </bean>

  <alias alias="LuLuRelationType-id" name="CoreAttributeReference-id" />  
  <alias alias="LuLuRelationType-createId" name="CoreAttributeReference-createId" />
  <alias alias="LuLuRelationType-createDate" name="CoreAttributeReference-createDate" />
  <alias alias="LuLuRelationType-updateId" name="CoreAttributeReference-updateId" />
  <alias alias="LuLuRelationType-updateDate" name="CoreAttributeReference-updateDate" />
  <alias alias="LuLuRelationType-activeFromDate" name="CoreAttributeReference-activeFromDate" />
  <alias alias="LuLuRelationType-activeToDate" name="CoreAttributeReference-activeToDate" />
  <alias alias="LuLuRelationType-active" name="CoreAttributeReference-active" />
  <alias alias="LuLuRelationType-description" name="CoreAttributeReference-description" />
  
  <bean id="LuLuRelationType-name" parent="LuLuRelationType-name-parentBean" />
  <bean id="LuLuRelationType-name-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
    <property name="name" value="name" />
    <property name="shortLabel" value="Name"/>
    <property name="summary" value="Name"/>
    <property name="label" value="Name"/>
    <property name="description" value="Name"/>
  </bean>
  
  <bean id="LuLuRelationType-reverseDescription" parent="LuLuRelationType-reverseDescription-parentBean" />
  <bean id="LuLuRelationType-reverseDescription-parentBean" abstract="true" parent="CoreAttributeReference-description">
    <property name="name" value="reverseDescription" />
    <property name="shortLabel" value="Reverse Description"/>
    <property name="summary" value="Reverse Description"/>
    <property name="label" value="Reverse Description"/>
    <property name="description" value="Reverse Description"/>
  </bean>
  
  <bean id="LuLuRelationType-reverseName" parent="LuLuRelationType-reverseName-parentBean" />
  <bean id="LuLuRelationType-reverseName-parentBean" abstract="true" parent="CoreAttributeReference-genericString">
    <property name="name" value="reverseName" />
    <property name="shortLabel" value="Reverse Name"/>
    <property name="summary" value="Reverse Name"/>
    <property name="label" value="Reverse Name"/>
    <property name="description" value="Reverse Name"/>
  </bean>


  <bean id="LuLuRelationType-lookupDefinition" parent="LuLuRelationType-lookupDefinition-parentBean" />
  <bean id="LuLuRelationType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="LU-LU Relation Type Lookup" />
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="name" />
        <bean parent="FieldDefinition" p:attributeName="reverseName" />
        <bean parent="FieldDefinition" p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="name" />
        <bean parent="FieldDefinition" p:attributeName="description" />
        <bean parent="FieldDefinition" p:attributeName="reverseName" />
        <bean parent="FieldDefinition" p:attributeName="reverseDescription" />
        <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
        <bean parent="FieldDefinition" p:attributeName="activeToDate" />
      </list>
    </property>
  </bean>

  <bean id="LuLuRelationType-inquiryDefinition" parent="LuLuRelationType-inquiryDefinition-parentBean" />
  <bean id="LuLuRelationType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="LU-LU Relation Type Inquiry" />
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Relation Type" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="id" />
              <bean parent="FieldDefinition" p:attributeName="name" />
              <bean parent="FieldDefinition" p:attributeName="description" />
              <bean parent="FieldDefinition" p:attributeName="reverseName" />
              <bean parent="FieldDefinition" p:attributeName="reverseDescription" />
              <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
              <bean parent="FieldDefinition" p:attributeName="activeToDate" />
            </list>
          </property>
        </bean>
        <ref bean="LuLuRelationType-inquirySection-luTypes" />
        <ref bean="LuLuRelationType-inquirySection-metaInformation" />
      </list>
    </property>
  </bean>
  
  <bean id="LuLuRelationType-inquirySection-luTypes" parent="LuLuRelationType-inquirySection-luTypes-parentBean" />
  <bean id="LuLuRelationType-inquirySection-luTypes-parentBean" abstract="true" parent="InquirySectionDefinition">
    <property name="title" value="Allowed LU Types" />
    <property name="numberOfColumns" value="1" />
    <property name="inquiryFields">
      <list>
        <bean parent="InquiryCollectionDefinition">
          <property name="attributeName" value="luTypes" />
          <property name="businessObjectClass"
              value="org.kuali.student.lum.lu.bo.LuType" />
          <property name="numberOfColumns" value="1" />
          <property name="summaryTitle" value="LU Type" />
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="id" />
              <bean parent="FieldDefinition" p:attributeName="name" />
              <bean parent="FieldDefinition" p:attributeName="description" />
            </list>
          </property>
          <property name="summaryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="name" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  
  <alias alias="LuLuRelationType-inquirySection-metaInformation" name="CoreAttributeReference-inquirySection-metaInformation" />

</beans>