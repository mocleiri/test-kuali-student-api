<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean id="CluAtpTypeKey" parent="CluAtpTypeKey-parentBean" />
  <bean id="CluAtpTypeKey-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.student.lum.lu.bo.CluAtpTypeKey" />
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="Offered ATP Types" />
    <property name="lookupDefinition">
      <ref bean="CluAtpTypeKey-lookupDefinition" />
    </property>
    <property name="inquiryDefinition">
      <ref bean="CluAtpTypeKey-inquiryDefinition" />
    </property>
    <property name="attributes">
      <list>
        <ref bean="CluAtpTypeKey-id" />
        <ref bean="CluAtpTypeKey-atpTypeKey" />
        <ref bean="CluAtpTypeKey-cluId" />
      </list>
    </property>
    <property name="relationships">
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="atpTypeKey"/>
          <property name="targetClass" value="org.kuali.student.core.atp.bo.AtpSeasonalType" />
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="atpTypeKey" p:targetName="id"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <alias alias="CluAtpTypeKey-id" name="CoreAttributeReference-id" />
  
  <bean id="CluAtpTypeKey-atpTypeKey" parent="CluAtpTypeKey-atpTypeKey-parentBean" />
  <bean id="CluAtpTypeKey-atpTypeKey-parentBean" abstract="true" parent="CoreAttributeReference-id">
    <property name="name" value="atpTypeKey" />
    <property name="shortLabel" value="ATP Type ID"/>
    <property name="summary" value="ATP Type ID"/>
    <property name="label" value="ATP Type ID"/>
    <property name="description" value="ATP Type ID"/>
  </bean>
  
  <bean id="CluAtpTypeKey-cluId" parent="CluAtpTypeKey-cluId-parentBean" />
  <bean id="CluAtpTypeKey-cluId-parentBean" abstract="true" parent="CoreAttributeReference-id">
    <property name="name" value="cluId" />
    <property name="shortLabel" value="CLU ID"/>
    <property name="summary" value="CLU ID"/>
    <property name="label" value="CLU ID"/>
    <property name="description" value="CLU ID"/>
  </bean>


  <bean id="CluAtpTypeKey-lookupDefinition" parent="CluAtpTypeKey-lookupDefinition-parentBean" />
  <bean id="CluAtpTypeKey-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Offered ATP Type Lookup" />
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="atpTypeKey" />
        <bean parent="FieldDefinition" p:attributeName="cluId" />
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="atpTypeKey" />
        <bean parent="FieldDefinition" p:attributeName="cluId" />
      </list>
    </property>
  </bean>

  <bean id="CluAtpTypeKey-inquiryDefinition" parent="CluAtpTypeKey-inquiryDefinition-parentBean" />
  <bean id="CluAtpTypeKey-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Offered ATP Type Inquiry" />
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Overview" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="id" />
              <bean parent="FieldDefinition" p:attributeName="atpTypeKey" />
              <bean parent="FieldDefinition" p:attributeName="cluId" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

</beans>