<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CluFee" parent="CluFee-parentBean" />
  <bean id="CluFee-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.student.lum.lu.bo.CluFee" />
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="CLU Fee" />
    <property name="lookupDefinition">
      <ref bean="CluFee-lookupDefinition" />
    </property>
    <property name="inquiryDefinition">
      <ref bean="CluFee-inquiryDefinition" />
    </property>
    <property name="attributes">
      <list>
        <ref bean="CluFee-id" />
        <ref bean="CluFee-createId" />
        <ref bean="CluFee-createDate" />
        <ref bean="CluFee-updateId" />
        <ref bean="CluFee-updateDate" />
        <ref bean="CluFee-description.plain" />        
      </list>
    </property>
  </bean>

  <alias alias="CluFee-id" name="CoreAttributeReference-id" />
  
  <alias alias="CluFee-createId" name="CoreAttributeReference-createId" />
  <alias alias="CluFee-createDate" name="CoreAttributeReference-createDate" />
  <alias alias="CluFee-updateId" name="CoreAttributeReference-updateId" />
  <alias alias="CluFee-updateDate" name="CoreAttributeReference-updateDate" />
  
  <bean id="CluFee-description.plain" parent="CluFee-description.plain-parentBean" />
  <bean id="CluFee-description.plain-parentBean" abstract="true" parent="CoreAttributeReference-richText.plain">
    <property name="name" value="description.plain"/>
    <property name="summary" value="Description for the fee"/>
    <property name="label" value="Description"/>
    <property name="description" value="Description for the fee"/>
  </bean>


  <bean id="CluFee-lookupDefinition" parent="CluFee-lookupDefinition-parentBean" />
  <bean id="CluFee-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="CLU Fee Lookup" />
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="description.plain" />
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="description.plain" />
      </list>
    </property>
  </bean>

  <bean id="CluFee-inquiryDefinition" parent="CluFee-inquiryDefinition-parentBean" />
  <bean id="CluFee-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="CLU Fee Inquiry" />
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Overview" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="id" />
              <bean parent="FieldDefinition" p:attributeName="description.plain" />
            </list>
          </property>
        </bean>
        <ref bean="CluFee-inquirySection-fee" />
        <ref bean="CluFee-inquirySection-metaInformation" />
      </list>
    </property>
  </bean>
  
  <bean id="CluFee-inquirySection-fee" parent="CluFee-inquirySection-fee-parentBean" />
  <bean id="CluFee-inquirySection-fee-parentBean" abstract="true" parent="InquirySectionDefinition">
    <property name="title" value="Fees" />
    <property name="numberOfColumns" value="1" />
    <property name="inquiryFields">
      <list>
        <bean parent="InquiryCollectionDefinition">
          <property name="attributeName" value="cluFeeRecords" />
          <property name="businessObjectClass"
              value="org.kuali.student.lum.lu.bo.CluFeeRecord" />
          <property name="numberOfColumns" value="1" />
          <property name="summaryTitle" value="Fee" />
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="id" />
              <bean parent="FieldDefinition" p:attributeName="feeType" />
              <bean parent="FieldDefinition" p:attributeName="rateType" />
              <bean parent="FieldDefinition" p:attributeName="description.plain" />
            </list>
          </property>
          <property name="summaryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="feeType" />
              <bean parent="FieldDefinition" p:attributeName="rateType" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  
  <alias alias="CluFee-inquirySection-metaInformation" name="CoreAttributeReference-inquirySection-metaInformation" />

</beans>