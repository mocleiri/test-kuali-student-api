  <bean id="Clu" parent="Clu-parentBean" />
  <bean id="Clu-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.student.lum.lu.bo.Clu" />
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="CLU" />
    <property name="lookupDefinition">
      <ref bean="Clu-lookupDefinition" />
    </property>
    <property name="inquiryDefinition">
      <ref bean="Clu-inquiryDefinition" />
    </property>
    <property name="attributes">
      <list>
        <ref bean="Clu-id" />
        <ref bean="Clu-createId" />
        <ref bean="Clu-createDate" />
        <ref bean="Clu-updateId" />
        <ref bean="Clu-updateDate" />
        <ref bean="Clu-versionIndId" />
        <ref bean="Clu-sequenceNumber" />
        <ref bean="Clu-currentVersionEnd" />
        <ref bean="Clu-currentVersionStart" />
        <ref bean="Clu-versionedFromId" />
        <ref bean="Clu-versionComment" />
        <ref bean="Clu-activeFromDate" />
        <ref bean="Clu-activeToDate" />
        <ref bean="Clu-studySubjectArea" />
        <ref bean="Clu-expectedFirstAtp" />
        <ref bean="Clu-lastAtp" />
        <ref bean="Clu-lastAdmitAtp" />
        <ref bean="Clu-referenceURL" />
        <ref bean="Clu-nextReviewPeriod" />
        <ref bean="Clu-defaultEnrollmentEstimate" />
        <ref bean="Clu-defaultMaximumEnrollment" />
        <ref bean="Clu-canCreateLui" />
        <ref bean="Clu-hazardousForDisabledStudents" />
        <ref bean="Clu-hasEarlyDropDeadline" />
        <ref bean="Clu-enrollable" />
        <ref bean="Clu-intensityTypeId" />
        <ref bean="Clu-intensityQuantity" />
        <ref bean="Clu-atpDurationTypeId" />
        <ref bean="Clu-timeQuantity" />
        <ref bean="Clu-typeId" />
        <ref bean="Clu-state" />
        <ref bean="Clu-description.plain" />
        <ref bean="Clu-luType.name" />
        <ref bean="Clu-officialCluId" />
        <ref bean="Clu-officialIdentifier.code" />
        <ref bean="Clu-primaryInstructor.personId" />
        <ref bean="Clu-fee.description.plain" />
      </list>
    </property>
    <property name="collections">
      <list>
        <bean parent="CollectionDefinition" p:name="alternateIdentifiers" p:shortLabel="Alternate Identifiers"
            p:summary="Alternate Identifiers" p:label="Alternate Identifiers"
            p:description="Alternate identifiers for this CLU." />
        <bean parent="CollectionDefinition" p:name="offeredAtpTypes" p:shortLabel="Offered ATP Types"
            p:summary="Offered ATP Types" p:label="Offered ATP Types"
            p:description="Offered ATP types for this CLU." />
      </list>
    </property>
    <property name="relationships">
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="expectedFirstAtp"/>
          <property name="targetClass" value="org.kuali.student.core.atp.bo.Atp" />
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="expectedFirstAtp" p:targetName="id"/>
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="lastAtp"/>
          <property name="targetClass" value="org.kuali.student.core.atp.bo.Atp" />
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="lastAtp" p:targetName="id"/>
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="lastAdmitAtp"/>
          <property name="targetClass" value="org.kuali.student.core.atp.bo.Atp" />
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="lastAdmitAtp" p:targetName="id"/>
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="nextReviewPeriod"/>
          <property name="targetClass" value="org.kuali.student.core.atp.bo.Atp" />
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="nextReviewPeriod" p:targetName="id"/>
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="atpDurationTypeId"/>
          <property name="targetClass" value="org.kuali.student.core.atp.bo.AtpDurationType" />
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="atpDurationTypeId" p:targetName="id"/>
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="intensityTypeId"/>
          <property name="targetClass" value="org.kuali.student.core.atp.bo.AtpDurationType" />
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="intensityTypeId" p:targetName="id"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

