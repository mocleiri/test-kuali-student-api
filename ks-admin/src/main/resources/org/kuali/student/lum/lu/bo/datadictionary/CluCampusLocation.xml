<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CluCampusLocation" parent="CluCampusLocation-parentBean" />
  <bean id="CluCampusLocation-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.student.lum.lu.bo.CluCampusLocation" />
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="Campus Location" />
    <property name="lookupDefinition">
      <ref bean="CluCampusLocation-lookupDefinition" />
    </property>
    <property name="inquiryDefinition">
      <ref bean="CluCampusLocation-inquiryDefinition" />
    </property>
    <property name="attributes">
      <list>
        <ref bean="CluCampusLocation-id" />
        <ref bean="CluCampusLocation-campusLocation" />
        <ref bean="CluCampusLocation-cluId" />
      </list>
    </property>
  </bean>

  <alias alias="CluCampusLocation-id" name="CoreAttributeReference-id" />
  
  <bean id="CluCampusLocation-campusLocation" parent="CluCampusLocation-campusLocation-parentBean" />
  <bean id="CluCampusLocation-campusLocation-parentBean" abstract="true" parent="CoreAttributeReference-id">
    <property name="name" value="campusLocation" />
    <property name="shortLabel" value="Campus ID"/>
    <property name="summary" value="Campus ID"/>
    <property name="label" value="Campus ID"/>
    <property name="description" value="Campus ID"/>
  </bean>
  
  <bean id="CluCampusLocation-cluId" parent="CluCampusLocation-cluId-parentBean" />
  <bean id="CluCampusLocation-cluId-parentBean" abstract="true" parent="CoreAttributeReference-id">
    <property name="name" value="cluId" />
    <property name="shortLabel" value="CLU ID"/>
    <property name="summary" value="CLU ID"/>
    <property name="label" value="CLU ID"/>
    <property name="description" value="CLU ID"/>
  </bean>


  <bean id="CluCampusLocation-lookupDefinition" parent="CluCampusLocation-lookupDefinition-parentBean" />
  <bean id="CluCampusLocation-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="CLU Campus Location Lookup" />
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="campusLocation" />
        <bean parent="FieldDefinition" p:attributeName="cluId" />
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="campusLocation" />
        <bean parent="FieldDefinition" p:attributeName="cluId" />
      </list>
    </property>
  </bean>

  <bean id="CluCampusLocation-inquiryDefinition" parent="CluCampusLocation-inquiryDefinition-parentBean" />
  <bean id="CluCampusLocation-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="CLU Campus Location Inquiry" />
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="CLU Campus" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="id" />
              <bean parent="FieldDefinition" p:attributeName="campusLocation" />
              <bean parent="FieldDefinition" p:attributeName="cluId" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

</beans>