<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean id="CluCluRelation" parent="CluCluRelation-parentBean" />
  <bean id="CluCluRelation-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.student.lum.lu.bo.CluCluRelation" />
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="CLU Relation" />
    <property name="lookupDefinition">
      <ref bean="CluCluRelation-lookupDefinition" />
    </property>
    <property name="inquiryDefinition">
      <ref bean="CluCluRelation-inquiryDefinition" />
    </property>
    <property name="attributes">
      <list>
        <ref bean="CluCluRelation-id" />
        <ref bean="CluCluRelation-createId" />
        <ref bean="CluCluRelation-createDate" />
        <ref bean="CluCluRelation-updateId" />
        <ref bean="CluCluRelation-updateDate" />
        <ref bean="CluCluRelation-activeFromDate" />
        <ref bean="CluCluRelation-activeToDate" />
        <ref bean="CluCluRelation-active" />        
        <ref bean="CluCluRelation-cluId" />
        <ref bean="CluCluRelation-relatedCluId" />
        <ref bean="CluCluRelation-luLuRelationTypeId" />
        <ref bean="CluCluRelation-cluRelationRequired" />
      </list>
    </property>
  </bean>

  <alias alias="CluCluRelation-id" name="CoreAttributeReference-id" />  
  <alias alias="CluCluRelation-createId" name="CoreAttributeReference-createId" />
  <alias alias="CluCluRelation-createDate" name="CoreAttributeReference-createDate" />
  <alias alias="CluCluRelation-updateId" name="CoreAttributeReference-updateId" />
  <alias alias="CluCluRelation-updateDate" name="CoreAttributeReference-updateDate" />
  <alias alias="CluCluRelation-activeFromDate" name="CoreAttributeReference-activeFromDate" />
  <alias alias="CluCluRelation-activeToDate" name="CoreAttributeReference-activeToDate" />
  <alias alias="CluCluRelation-active" name="CoreAttributeReference-active" />
  <alias alias="CluCluRelation-description" name="CoreAttributeReference-description" />
  
  <bean id="CluCluRelation-cluId" parent="CluCluRelation-cluId-parentBean"/>
  <bean id="CluCluRelation-cluId-parentBean" abstract="true" parent="CoreAttributeReference-id">
    <property name="name" value="cluId"/>
    <property name="shortLabel" value="CLU ID"/>
    <property name="summary" value="CLU ID"/>
    <property name="label" value="CLU ID"/>
    <property name="description" value="CLU ID"/>
  </bean>
  
  <bean id="CluCluRelation-relatedCluId" parent="CluCluRelation-relatedCluId-parentBean"/>
  <bean id="CluCluRelation-relatedCluId-parentBean" abstract="true" parent="CoreAttributeReference-id">
    <property name="name" value="relatedCluId"/>
    <property name="shortLabel" value="Related CLU ID"/>
    <property name="summary" value="Related CLU ID"/>
    <property name="label" value="Related CLU ID"/>
    <property name="description" value="Related CLU ID"/>
  </bean>
  
  <bean id="CluCluRelation-luLuRelationTypeId" parent="CluCluRelation-luLuRelationTypeId-parentBean"/>
  <bean id="CluCluRelation-luLuRelationTypeId-parentBean" abstract="true" parent="CoreAttributeReference-id">
    <property name="name" value="luLuRelationTypeId"/>
    <property name="shortLabel" value="LU-LU Relation Type ID"/>
    <property name="summary" value="LU-LU Relation Type ID"/>
    <property name="label" value="LU-LU Relation Type ID"/>
    <property name="description" value="LU-LU Relation Type ID"/>
    <property name="control">
      <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.student.lum.lu.bo.options.LuLuRelationTypeValuesFinder"/>
    </property>
  </bean>
  
  <bean id="CluCluRelation-cluRelationRequired" parent="CluCluRelation-cluRelationRequired-parentBean" />
  <bean id="CluCluRelation-cluRelationRequired-parentBean" abstract="true" parent="CoreAttributeReference-genericBoolean">
    <property name="name" value="cluRelationRequired"/>
    <property name="shortLabel" value="CLU Relation Required"/>
    <property name="summary" value="CLU Relation Required"/>
    <property name="label" value="CLU Relation Required"/>
    <property name="description" value="CLU Relation Required"/>
  </bean>


  <bean id="CluCluRelation-lookupDefinition" parent="CluCluRelation-lookupDefinition-parentBean" />
  <bean id="CluCluRelation-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="CLU Relation Lookup" />
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="cluId" />
        <bean parent="FieldDefinition" p:attributeName="relatedCluId" />
        <bean parent="FieldDefinition" p:attributeName="luLuRelationTypeId" />
        <bean parent="FieldDefinition" p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="cluId" />
        <bean parent="FieldDefinition" p:attributeName="relatedCluId" />
        <bean parent="FieldDefinition" p:attributeName="luLuRelationTypeId" />
        <bean parent="FieldDefinition" p:attributeName="cluRelationRequired" />
        <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
        <bean parent="FieldDefinition" p:attributeName="activeToDate" />
      </list>
    </property>
  </bean>

  <bean id="CluCluRelation-inquiryDefinition" parent="CluCluRelation-inquiryDefinition-parentBean" />
  <bean id="CluCluRelation-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="CLU Relation Inquiry" />
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Overview" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="id" />
              <bean parent="FieldDefinition" p:attributeName="cluId" />
              <bean parent="FieldDefinition" p:attributeName="relatedCluId" />
              <bean parent="FieldDefinition" p:attributeName="luLuRelationTypeId" />
              <bean parent="FieldDefinition" p:attributeName="cluRelationRequired" />
              <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
              <bean parent="FieldDefinition" p:attributeName="activeToDate" />
            </list>
          </property>
        </bean>
        <ref bean="CluCluRelation-inquirySection-metaInformation" />
      </list>
    </property>
  </bean>
  
  <alias alias="CluCluRelation-inquirySection-metaInformation" name="CoreAttributeReference-inquirySection-metaInformation" />

</beans>