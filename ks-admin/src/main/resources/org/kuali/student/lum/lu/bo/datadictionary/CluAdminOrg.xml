<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean id="CluAdminOrg" parent="CluAdminOrg-parentBean" />
  <bean id="CluAdminOrg-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.student.lum.lu.bo.CluAdminOrg" />
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="CLU Admin Organization" />
    <property name="lookupDefinition">
      <ref bean="CluAdminOrg-lookupDefinition" />
    </property>
    <property name="inquiryDefinition">
      <ref bean="CluAdminOrg-inquiryDefinition" />
    </property>
    <property name="attributes">
      <list>
        <ref bean="CluAdminOrg-id" />
        <ref bean="CluAdminOrg-orgId" />
        <ref bean="CluAdminOrg-type" />
        <ref bean="CluAdminOrg-cluId" />
        <ref bean="CluAdminOrg-primary" />
      </list>
    </property>
  </bean>

  <alias alias="CluAdminOrg-id" name="CoreAttributeReference-id" />
  <alias alias="CluAdminOrg-orgId" name="CoreAttributeReference-orgId" />
  
  <bean id="CluAdminOrg-type" parent="CluAdminOrg-type-parentBean" />
  <bean id="CluAdminOrg-type-parentBean" abstract="true" parent="CoreAttributeReference-id">
    <property name="name" value="type" />
    <property name="shortLabel" value="Type"/>
    <property name="summary" value="Type"/>
    <property name="label" value="Type"/>
    <property name="description" value="Type"/>
  </bean>
  
  <bean id="CluAdminOrg-cluId" parent="CluAdminOrg-cluId-parentBean" />
  <bean id="CluAdminOrg-cluId-parentBean" abstract="true" parent="CoreAttributeReference-id">
    <property name="name" value="cluId" />
    <property name="shortLabel" value="CLU ID"/>
    <property name="summary" value="CLU ID"/>
    <property name="label" value="CLU ID"/>
    <property name="description" value="CLU ID"/>
  </bean>
  
  <bean id="CluAdminOrg-primary" parent="CluAdminOrg-primary-parentBean" />
  <bean id="CluAdminOrg-primary-parentBean" abstract="true" parent="CoreAttributeReference-genericBoolean">
    <property name="name" value="primary" />
    <property name="shortLabel" value="Primary"/>
    <property name="summary" value="Primary Indicator"/>
    <property name="label" value="Primary Indicator"/>
    <property name="description" value="Primary Indicator"/>
  </bean>


  <bean id="CluAdminOrg-lookupDefinition" parent="CluAdminOrg-lookupDefinition-parentBean" />
  <bean id="CluAdminOrg-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="CLU Identifier Lookup" />
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="orgId" />
        <bean parent="FieldDefinition" p:attributeName="type" />
        <bean parent="FieldDefinition" p:attributeName="primary" />
        <bean parent="FieldDefinition" p:attributeName="cluId" />
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" />
        <bean parent="FieldDefinition" p:attributeName="orgId" />
        <bean parent="FieldDefinition" p:attributeName="type" />
        <bean parent="FieldDefinition" p:attributeName="primary" />
        <bean parent="FieldDefinition" p:attributeName="cluId" />
      </list>
    </property>
  </bean>

  <bean id="CluAdminOrg-inquiryDefinition" parent="CluAdminOrg-inquiryDefinition-parentBean" />
  <bean id="CluAdminOrg-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="CLU Admin Organization Inquiry" />
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Overview" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="id" />
              <bean parent="FieldDefinition" p:attributeName="orgId" />
              <bean parent="FieldDefinition" p:attributeName="type" />
              <bean parent="FieldDefinition" p:attributeName="primary" />
              <bean parent="FieldDefinition" p:attributeName="cluId" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

</beans>