<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="SubjectCodeType" parent="SubjectCodeType-parentBean" />
    <bean id="SubjectCodeType-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.student.core.subjectcode.bo.SubjectCodeType" />
        <property name="titleAttribute" value="id" />
        <property name="objectLabel" value="Subject Code Type" />
        <property name="lookupDefinition">
            <ref bean="SubjectCodeType-lookupDefinition" />
        </property>
        <property name="inquiryDefinition">
            <ref bean="SubjectCodeType-inquiryDefinition" />
        </property>
        <property name="attributes">
            <list>
                <ref bean="SubjectCodeType-id" />
                <ref bean="SubjectCodeType-name" />
                <ref bean="SubjectCodeType-description" />
                <ref bean="SubjectCodeType-activeFromDate" />
                <ref bean="SubjectCodeType-activeToDate" />
                <ref bean="SubjectCodeType-active" />
            </list>
        </property>
    </bean>

    <alias alias="SubjectCodeType-id" name="CoreTypeReference-id" />
    <alias alias="SubjectCodeType-name" name="CoreTypeReference-name" />
    <alias alias="SubjectCodeType-description" name="CoreTypeReference-description" />
    <alias alias="SubjectCodeType-activeFromDate" name="CoreTypeReference-activeFromDate" />
    <alias alias="SubjectCodeType-activeToDate" name="CoreTypeReference-activeToDate" />
    <alias alias="SubjectCodeType-active" name="CoreTypeReference-active" />


    <bean id="SubjectCodeType-lookupDefinition" parent="SubjectCodeType-lookupDefinition-parentBean" />
    <bean id="SubjectCodeType-lookupDefinition-parentBean" abstract="true" parent="CoreTypeReference-lookupDefinition">
    	<property name="title" value="Subject Code Type Lookup" />
    </bean>
    

    <bean id="SubjectCodeType-inquiryDefinition" parent="SubjectCodeType-inquiryDefinition-parentBean" />
    <bean id="SubjectCodeType-inquiryDefinition-parentBean" abstract="true" parent="CoreTypeReference-inquiryDefinition">
        <property name="title" value="Subject Code Type Inquiry" />
    </bean>
    
</beans>