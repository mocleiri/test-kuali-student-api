      </plugin>-->
      <plugin>
        <groupId>org.apache.cxf</groupId>
        <artifactId>cxf-java2ws-plugin</artifactId>
        <version>2.3.1</version>
        <executions>
          <execution>
            <id>luiPersonRelationService-wsdl</id>
            <phase>${ks.java2ws.phase}</phase>
            <configuration>
              <className>org.kuali.student.enrollment.lpr.service.LuiPersonRelationService</className>
              <genWsdl>true</genWsdl>
              <verbose>true</verbose>
            </configuration>
            <goals>
              <goal>java2ws</goal>
            </goals>
          </execution>
          <execution>
            <id>atpService-wsdl</id>
            <phase>${ks.java2ws.phase}</phase>
            <configuration>
              <className>org.kuali.student.core.atp.service.AtpService</className>
              <genWsdl>true</genWsdl>
              <verbose>true</verbose>
            </configuration>
            <goals>
              <goal>java2ws</goal>
            </goals>
          </execution>  
          <execution>
            <id>academicCalendarService-wsdl</id>
            <phase>${ks.java2ws.phase}</phase>
            <configuration>
              <className>org.kuali.student.core.academiccalendar.service.AcademicCalendarService</className>
              <genWsdl>true</genWsdl>
              <verbose>true</verbose>
            </configuration>
            <goals>
              <goal>java2ws</goal>
            </goals>
          </execution>            
          <execution>
            <id>luiService-wsdl</id>
            <phase>${ks.java2ws.phase}</phase>
            <configuration>
              <className>org.kuali.student.enrollment.lui.service.LuiService</className>
              <genWsdl>true</genWsdl>
              <verbose>true</verbose>
            </configuration>
            <goals>
              <goal>java2ws</goal>
            </goals>
          </execution>           
        </executions>
      </plugin>
    </plugins>
  </build>

