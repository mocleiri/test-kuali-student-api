<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <import resource="classpath:ks-dictionary-file-locations.xml"/>

    <bean id="dataDictionaryServiceForLpr" class="org.kuali.student.datadictionary.DataDictionaryServiceImpl">
        <property name="serviceNamespaceSuffix" value="luiPersonRelation" />
        <property name="dictionaryLocations">
            <list>
                <bean parent="lprDictionaryLocation" />
            </list>
        </property>
    </bean>

    <bean id="dataDictionaryServiceForAllUsedByRice" class="org.kuali.student.datadictionary.DataDictionaryServiceImpl">
        <property name="serviceNamespaceSuffix" value="shouldNotMatter" />
        <property name="dictionaryLocations" ref="allDictionaryFileLocations" />
    </bean>
</beans>
