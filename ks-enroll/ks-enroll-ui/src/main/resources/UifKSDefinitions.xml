    <bean id="KSFormView" abstract="true" parent="FormView">
        <property name="additionalCssFiles" ref="ks-css" />
    </bean>

    <bean id="KSLookupView" abstract="true" parent="LookupView">
        <property name="additionalCssFiles" ref="ks-css" />
    </bean>

    <bean id="KSInquiryView" abstract="true" parent="InquiryView">
        <property name="additionalCssFiles" ref="ks-css" />
    </bean>

    <bean id="KSMaintenanceView" abstract="true" parent="MaintenanceView">
        <property name="additionalCssFiles" ref="ks-css" />
    </bean>

    <bean id="ScheduleControl" parent="ScheduleControl-parentBean"/>
    <bean id="ScheduleControl-parentBean" abstract="true" class="org.kuali.student.enrollment.uif.control.ScheduleControl" scope="prototype">
          <property name="template" value="/ks-enroll/WEB-INF/jsp/schedule.jsp"/>
          <property name="sundayFirst" value="false"/>
          <property name="startTime" value="5"/>
          <property name="endTime" value="20"/>
          <property name="dayType" value="ONE_LETTER"/>
          <property name="useLetterKeys" value="false"/>
          <property name="omitWeekend" value="false"/>
          <property name="omitAMPMSuffix" value="false"/>
          <property name="militaryTime" value="false"/>
    </bean>

</beans>