  <!-- LookupView -->
  <bean id="TermWrapper-LookupView" parent="LookupView">
    <property name="title" value="Term Lookup (by Key)" />
    <property name="dataObjectClassName" value="org.kuali.student.enrollment.class2.acal.dto.TermWrapper" />
<!--     <property name="viewHelperServiceClassName" value="org.kuali.student.enrollment.class2.acal.service.TermInfoLookupViewHelperServiceImpl" /> -->
    
    <property name="criteriaFields">
      <list>
        <bean parent="LookupCriteriaAttributeField" p:propertyName="termInfo.key" />
        <bean parent="LookupCriteriaAttributeField" p:propertyName="termInfo.typeKey"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
		<bean parent="AttributeField" p:propertyName="termInfo.key" />
        <bean parent="AttributeField" p:propertyName="termInfo.typeKey" /> 		
        <bean parent="AttributeField" p:propertyName="termInfo.startDate" />
        <bean parent="AttributeField" p:propertyName="termInfo.endDate" />       
      </list>
    </property>
  </bean>
  
  <!-- InquiryView -->
  <bean id="TermWrapper-InquiryView" parent="InquiryView">
     <property name="title" value="Term with Key Dates Inquiry"/>
     <property name="dataObjectClassName" value="org.kuali.student.enrollment.class2.acal.dto.TermWrapper"/>
<!--      <property name="viewHelperServiceClassName" value="org.kuali.student.enrollment.class2.acal.service.TermWrapperInquiryViewHelperServiceImpl" />-->
     
     <property name="Items">
         <list>
           <bean parent="GroupSectionGridLayout">
             <property name="layoutManager.numberOfColumns" value="2"/>
             <property name="title" value="Term with Key Dates"/>
             <property name="items">
               <list>
               	<bean parent="AttributeField" p:propertyName="termInfo.key"/>
                <bean parent="AttributeField" p:propertyName="termInfo.typeKey"/>
                <bean parent="AttributeField" p:propertyName="termInfo.startDate"/>
                <bean parent="AttributeField" p:propertyName="termInfo.endDate"/>
                <bean parent="MessageField" p:messageText="Term Dates" p:colSpan="2" /> 
				<bean parent="MessageField" p:messageText="Dates between which classes should meet" p:colSpan="2" />
				<bean parent="AttributeField" p:propertyName="classesMeetDates.startDate"/>
                <bean parent="AttributeField" p:propertyName="classesMeetDates.endDate"/>  
                <bean parent="MessageField" p:messageText="Registration Period" p:colSpan="2"/>
                <bean parent="AttributeField" p:propertyName="registrationPeriod.startDate"/>
                <bean parent="AttributeField" p:propertyName="registrationPeriod.endDate"/>  
                <bean parent="MessageField" p:messageText="Drop Period ends" p:colSpan="2"/>
                <bean parent="AttributeField" p:propertyName="dropPeriodEndsDate.endDate">
                	<property name="label" value="Date" />
                </bean>
				<bean parent="MessageField" p:messageText="Final Examinations" p:colSpan="2"/> 
                <bean parent="AttributeField" p:propertyName="finalExaminationsDates.startDate"/>
                <bean parent="AttributeField" p:propertyName="finalExaminationsDates.endDate"/>
			    <bean parent="MessageField" p:messageText="Grades Due" p:colSpan="2"/> 
                <bean parent="AttributeField" p:propertyName="gradesDueDate.endDate">
               		<property name="label" value="Date" />
                </bean>   
               </list>
             </property>             
           </bean>
         </list>
     </property>
  </bean>
 
   
  <bean id="TermWrapper-MaintenanceView" parent="MaintenanceView">
     <property name="title" value="Term Wrapper Maintenance"/>
     <property name="dataObjectClassName" value="org.kuali.student.enrollment.class2.acal.dto.TermWrapper"/>
     <property name="viewHelperServiceClassName" value="org.kuali.student.enrollment.class2.acal.service.TermWrapperMaintainableImpl" />
    
     <property name="items">
       <list merge="true">
          <bean parent="MaintenanceGroupSectionGridLayout">
          	 <property name="title" value="Term Wrapper"/>
             <property name="items">
               <list>
                 <bean parent="AttributeField" p:propertyName="termInfo.typeKey">
                    <property name="control">
              			<bean parent="SelectControl"/>
            		</property>
            		<property name="optionsFinder">
              			<bean class="org.kuali.student.enrollment.class2.acal.keyvalue.TermInfoTypeKeyValues"/>
            		</property>
                </bean>
                <bean parent="AttributeField" p:propertyName="termInfo.startDate"/>
                <bean parent="AttributeField" p:propertyName="termInfo.endDate"/>

				<bean parent="Header" p:headerText="Term Dates" p:colSpan="2"/> 
				<bean parent="MessageField" p:messageText="Dates between which classes should meet" p:colSpan="2"/>
				<!-- <bean parent="BlankField"/> -->
                <bean parent="AttributeField" p:propertyName="classesMeetDates.startDate"/>
                <bean parent="AttributeField" p:propertyName="classesMeetDates.endDate"/>  
                
                <bean parent="MessageField" p:messageText="Registration Period" p:colSpan="2"/>
                <bean parent="AttributeField" p:propertyName="registrationPeriod.startDate"/>
                <bean parent="AttributeField" p:propertyName="registrationPeriod.endDate"/>  

				<bean parent="MessageField" p:messageText="Drop Period ends" p:colSpan="2"/>
                <bean parent="AttributeField" p:propertyName="dropPeriodEndsDate.endDate">
                	 <property name="label" value="Date" />
                </bean>

				<bean parent="MessageField" p:messageText="Final Examinations" p:colSpan="2"/> 
                <bean parent="AttributeField" p:propertyName="finalExaminationsDates.startDate"/>
                <bean parent="AttributeField" p:propertyName="finalExaminationsDates.endDate"/>
                
				<bean parent="MessageField" p:messageText="Grades Due" p:colSpan="2"/> 
                <bean parent="AttributeField" p:propertyName="gradesDueDate.endDate">
                	<property name="label" value="Date" />
                </bean>                                     
                                
               </list>
             </property>             
          </bean> 
       </list>
     </property>
  </bean>  


  <bean id="TermWrapperMaintenanceDocument" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.student.enrollment.class2.acal.dto.TermWrapper"/>
    <property name="maintainableClass" value="org.kuali.student.enrollment.class2.acal.service.TermWrapperMaintainableImpl"/>
    <property name="documentTypeName" value="TermWrapperMaintenanceDocument"/>
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
    <property name="lockingKeys">
      <list>
        <value>termInfo.key</value>
      </list>
    </property>
  </bean>  
</beans>   
  