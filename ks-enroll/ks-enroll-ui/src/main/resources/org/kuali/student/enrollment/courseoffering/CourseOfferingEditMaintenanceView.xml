    <bean id="CourseOfferingEditWrapper" parent="CourseOfferingEditWrapper-parentBean"/>

    <bean id="CourseOfferingEditWrapper-parentBean" abstract="true" parent="DataObjectEntry">
        <property name="dataObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.CourseOfferingEditWrapper"/>
        <property name="titleAttribute" value="id"/>
        <property name="attributes">
            <list>
            </list>
        </property>
        <property name="objectLabel" value="Course Offering"/>
        <property name="primaryKeys">
            <list>
                <value>coInfo.id</value>
            </list>
        </property>
    </bean>

    <!-- InquiryView -->
    <bean id="KS-CourseOfferingEditWrapper-InquiryView" parent="KSInquiryView"
          p:headerText="Course Offering Inquiry"
          p:dataObjectClassName="org.kuali.student.enrollment.class2.courseoffering.dto.CourseOfferingEditWrapper"
          p:viewHelperServiceClass="org.kuali.student.enrollment.class2.courseoffering.service.impl.CourseOfferingEditInquirableImpl"
        >
        <property name="page.header">
            <!-- suppress Expand/Collapse buttons -->
            <ref bean="KSHeader_Empty" />
        </property>
        <property name="Items">
            <list>
                <bean parent="Uif-VerticalBoxSection">
                    <property name="items">
                        <list>

                            <bean parent="Uif-GridGroup"
                                  p:layoutManager.numberOfColumns="2"
                                  p:layoutManager.applyDefaultCellWidths="false">
                                <property name="items">
                                    <list>
                                        <!--<bean parent="Uif-DataField" p:label="ID" p:propertyName="coInfo.id" />-->
                                        <bean parent="Uif-DataField" p:propertyName="coInfo.courseOfferingCode" />
                                        <bean parent="Uif-DataField" p:propertyName="coInfo.courseOfferingTitle" />
                                        <bean parent="Uif-DataField" p:propertyName="coInfo.termId" />
                                        <bean parent="Uif-DataField" p:propertyName="coInfo.creditCnt" />

                                        <!--<bean parent="Uif-DataField" p:label="Grading Option" p:propertyName="selectedGradingOptionName" />-->
                                        <bean parent="Uif-InputField" p:propertyName="coInfo.gradingOptionId" p:label="Grading Options">
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.GradingOptionsKeyValues" />
                                            </property>
                                        </bean>

                                        <!-- displays "[]" when empty, not desired
                                        <bean parent="Uif-InputField" p:propertyName="coInfo.studentRegistrationOptionIds" p:label="Student Registration Options" p:render="@{#dp.studentRegOptions.empty}">
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.StudentRegistrationOptionsKeyValues" p:addBlankOption="false"/>
                                            </property>
                                        </bean>
                                        -->
                                        <bean parent="Uif-DataField" p:propertyName="selectedStudentRegOpts"
                                              p:label="Student Registration Options" />

                                        <!--<bean parent="Uif-DataField" p:label="Final Exam" p:propertyName="finalExam" />-->
                                        <bean parent="Uif-DataField" p:propertyName="coInfo.finalExamType" />

                                        <!--<bean parent="Uif-Message" p:label="Has waitlist" p:messageText="@{dataObject.coInfo.hasWaitlist ? 'YES':'NO'}" />-->
                                        <bean parent="Uif-InputField" p:propertyName="coInfo.waitlistLevelTypeKey" p:label="Waitlist Level">
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.WaitlistLevelOptionsKeyValues" />
                                            </property>
                                        </bean>
                                        <bean parent="Uif-InputField" p:label="Waitlist Type" p:propertyName="coInfo.waitlistTypeKey">
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.WaitlistTypeOptionsKeyValues" />
                                            </property>
                                        </bean>

                                        <bean parent="Uif-MessageField" p:label="Honors Flag"
                                              p:messageText="@{(dataObject.coInfo.isHonorsOffering eq null or !dataObject.coInfo.isHonorsOffering) ? 'NO':'YES'}" />

                                        <!-- PO/UX does not currently want Delivery Formats for inquiry
                                        <bean  parent="Uif-StackedCollectionSection">
                                            <property name="headerText" value="Delivery Formats" />
                                            <property name="header">
                                                <bean parent="Uif-HeaderFive"/>
                                            </property>
                                            <property name="propertyName" value="formatOfferingList" />
                                            <property name="collectionObjectClass" value="org.kuali.student.enrollment.courseoffering.dto.FormatOfferingInfo" />
                                            <property name="layoutManager.lineGroupPrototype.header.render" value="false" />
                                            <property name="layoutManager.lineGroupPrototype.footer.render" value="false" />
                                            <property name="colSpan" value="2"/>
                                            <property name="cssClasses">
                                                <list merge="true">
                                                    <value>ks-collection-margin-bottom-zero</value>
                                                </list>
                                            </property>
                                            <property name="Items">
                                                <list>
                                                    <bean parent="Uif-DataField" p:propertyName="Name" p:label="Format Name" />
                                                    <bean parent="Uif-InputField" p:propertyName="gradeRosterLevelTypeKey"
                                                          p:label="Grade Roster Level">
                                                        <property name="optionsFinder">
                                                            <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.GradeRosterLevelKeyValues"/>
                                                        </property>
                                                    </bean>
                                                    <bean parent="Uif-InputField" p:propertyName="finalExamLevelTypeKey"
                                                          p:label="Final Exam Driver" p:render="@{#dp.coInfo.finalExamType eq 'STANDARD'}">
                                                        <property name="optionsFinder">
                                                            <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.FinalExamDriverKeyValues"/>
                                                        </property>
                                                    </bean>
                                                    <bean parent="Uif-Message" p:label="Final Exam Driver"
                                                          p:messageText="Alternate exam for this offering"
                                                          p:render="@{#dp.coInfo.finalExamType eq 'ALTERNATE'}" />
                                                    <bean parent="Uif-Message" p:label="Final Exam Driver"
                                                          p:messageText="No final exam for this offering"
                                                          p:render="@{#dp.coInfo.finalExamType eq 'NONE'}" />
                                                </list>
                                            </property>
                                        </bean>
                                        -->

                                        <!-- PO/UX does not currently want Affiliated Personnel for inquiry
                                        <bean  parent="Uif-TableCollectionSection" p:layoutManager.style="width: 100%; display:block;">
                                            <property name="headerText" value="Affiliated Personnel" />
                                            <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.OfferingInstructorWrapper" />
                                            <property name="propertyName" value="instructors" />
                                            <property name="layoutManager.renderSequenceField" value="false" />
                                            <property name="layoutManager.separateAddLine" value="false" />
                                            <property name="layoutManager.richTable.render" value="false" />
                                            <property name="layoutManager.applyDefaultCellWidths" value="false" />
                                            <property name="layoutManager.applyAlternatingRowStyles" value="false" />
                                            <property name="Items">
                                                <list>
                                                    <bean parent="Uif-DataField" p:label="Affiliated Personnel ID" p:propertyName="offeringInstructorInfo.personId" />
                                                    <bean parent="Uif-DataField" p:label="Affiliated Personnel Name" p:propertyName="offeringInstructorInfo.personName" />
                                                    <bean parent="Uif-DataField" p:label="Affiliated Personnel Affiliation" p:propertyName="typeName" />
                                                </list>
                                            </property>
                                        </bean>
                                        -->

                                        <!-- PO/UX does not currently want Admin Organizations for inquiry
                                        <bean parent="Uif-StackedCollectionSection">
                                            <property name="propertyName" value="organizationNames"/>
                                            <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.OrganizationInfoWrapper"/>
                                            <property name="headerText" value="Administering Organizations"/>
                                            <property name="header">
                                                <bean parent="Uif-HeaderFive"/>
                                            </property>
                                            <property name="layoutManager.lineGroupPrototype.header.render" value="false" />
                                            <property name="layoutManager.lineGroupPrototype.footer.render" value="false" />
                                            <property name="layoutManager.lineGroupPrototype.style" value="margin-bottom:0" />
                                            <property name="colSpan" value="2"/>
                                            <property name="cssClasses">
                                                <list merge="true">
                                                    <value>ks-collection-margin-bottom-zero</value>
                                                </list>
                                            </property>
                                            <property name="items">
                                                <list>
                                                    <bean parent="Uif-DataField" p:propertyName="organizationName" p:label="Organization Name"/>
                                                </list>
                                            </property>
                                        </bean>
                                        -->

                                    </list>
                                </property>
                            </bean>
                            <!-- end of Uif-GridGroup -->

                        </list>
                    </property>
                </bean>
                <!-- end of Uif-VerticalBoxSection -->

            </list>
        </property>
    </bean>

    <!-- Edit a Course Offering MaintenanceView -->
    <bean id="KS-CourseOfferingEditWrapper-EditMaintenanceView" parent="KSMaintenanceView">
        <property name="headerText" value="Edit @{document.oldMaintainableObject.dataObject.coInfo.courseOfferingCode} - @{document.oldMaintainableObject.dataObject.coInfo.courseOfferingTitle} (@{document.oldMaintainableObject.dataObject.coInfo.creditCnt} Credits)"/>
        <property name="dataObjectClassName" value="org.kuali.student.enrollment.class2.courseoffering.dto.CourseOfferingEditWrapper"/>

        <property name="viewHelperServiceClass" value="org.kuali.student.enrollment.class2.courseoffering.service.impl.CourseOfferingEditMaintainableImpl" />
        <property name="items">
            <list>
                <ref bean="KS-CourseOfferingEdit-Term"/>
                <ref bean="KSDocumentPageFooter_SubmitCancel"/>
                <ref bean="KS-CourseOfferingEdit-CourseOfferingInformation"/>
                <ref bean="KS-CourseOfferingEdit-DeliveryAndAssessment"/>
                <ref bean="KS-CourseOfferingEdit-Waitlist"/>
                <ref bean="KS-CourseOfferingEdit-PersonnelSection"/>
                <ref bean="KS-CourseOfferingEdit-MiscSection"/>

            </list>
        </property>
        <property name="page.footer.items">
            <ref bean="KSDocumentPageFooter_SubmitCancel" />
        </property>
    </bean>

    <bean id="KS-CourseOfferingEdit-Term" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <bean parent="Uif-DataField" p:label="Term" p:propertyName="termStartEnd"/>
            </list>
        </property>
    </bean>

    <bean id="KS-CourseOfferingEdit-CourseOfferingInformation" parent="Uif-VerticalBoxSection" p:headerText="Course Offering Information">
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="&lt;hr&gt;"/>
                <bean parent="Uif-HorizontalBoxSubSection">
                    <property name="items">
                        <list>
                            <bean parent="Uif-DataField" p:propertyName="course.code" p:label="Course Offering Code" p:fieldLabel.renderColon="false" p:labelPlacement="TOP" />
                            <bean parent="Uif-InputField" p:propertyName="coInfo.courseNumberSuffix" p:label="Change Suffix" p:fieldLabel.renderColon="false" p:labelPlacement="TOP">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="5" />
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <ref bean="KS-GradingAndRegOptions"/>
            </list>
        </property>
    </bean>


    <bean id="KS-GradingAndRegOptions" parent="Uif-HorizontalBoxSubSection">
        <property name="items">
            <list>
                <ref bean="KS-CourseOfferingEdit-GradingOptions" />
                <ref bean="KS-CourseOfferingEdit-StudentRegOptions" />
                <ref bean="KS-CourseOfferingEdit-CreditType" />
            </list>
        </property>
    </bean>
    <bean id="KS-CourseOfferingEdit-GradingOptions" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <bean parent="Uif-InputField" p:propertyName="coInfo.gradingOptionId" p:label="Grading Options" p:fieldLabel.renderColon="false" p:labelPlacement="TOP">
                    <property name="control">
                        <bean parent="Uif-VerticalRadioControl"/>
                    </property>
                    <property name="optionsFinder">
                        <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.GradingOptionsKeyValues" p:addBlankOption="false" />
                    </property>
                </bean>
            </list>
        </property>
    </bean>
    <bean id="KS-CourseOfferingEdit-StudentRegOptions" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <bean parent="Uif-InputField-LabelTop" p:propertyName="coInfo.studentRegistrationGradingOptions"
                      p:label="Student Registration Options" p:render="@{!#dp.studentRegOptions.empty}">
                    <property name="control">
                        <bean parent="Uif-VerticalCheckboxesControl"/>
                    </property>
                    <property name="optionsFinder">
                        <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.StudentRegistrationOptionsKeyValues"
                              p:addBlankOption="false" />
                    </property>
                </bean>
                <bean parent="Uif-MessageField" p:label="Student Registration Options"
                      p:labelPlacement="TOP" p:fieldLabel.renderColon="false"
                      p:messageText="None available" p:render="@{#dp.studentRegOptions.empty}"/>
            </list>
        </property>
    </bean>
    <bean id="KS-CourseOfferingEdit-CreditType" parent="Uif-VerticalBoxSection"
          p:style="margin-left:15px;">
        <property name="items">
            <list>
                <!-- Credit Option Type Selector -->
                <bean parent="Uif-InputField-LabelTop" p:propertyName="creditOption.typeKey" p:label="Credit Type">
                    <property name="control">
                        <bean parent="Uif-HorizontalRadioControl" p:disabled="@{#dp.creditOptionFixed}" />
                    </property>
                    <property name="optionsFinder">
                        <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.CreditOptionsKeyValues" p:addBlankOption="false" />
                    </property>
                </bean>
                <!-- Fixed Type -->
                <bean parent="Uif-InputField" p:label="Credits" p:propertyName="creditOption.fixedCredit" p:fieldLabel.renderColon="false"
                      p:style="margin-left: 1em;" p:progressiveRender="@{#dp.creditOption.typeKey eq 'kuali.result.values.group.type.fixed'}"
                      p:exclusiveMin="@{document.oldMaintainableObject.dataObject.creditOption.courseMinCredits}-0.0001"
                      p:inclusiveMax="@{document.oldMaintainableObject.dataObject.creditOption.courseMaxCredits}"  >
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="3" p:disabled="@{#dp.creditOptionFixed}" />
                    </property>
                </bean>
                <!-- Range -->
                <bean parent="Uif-HorizontalBoxSection" p:style="margin-left: 1em;" p:progressiveRender="@{#dp.creditOption.typeKey eq 'kuali.result.values.group.type.range'}">
                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField" p:label="Minimum Credits" p:exclusiveMin="@{document.oldMaintainableObject.dataObject.creditOption.courseMinCredits}-0.0001"
                                  p:propertyName="creditOption.minCredits" p:fieldLabel.renderColon="false" p:required="true">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="3" />
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:label="Maximum Credits" p:inclusiveMax="@{document.oldMaintainableObject.dataObject.creditOption.courseMaxCredits}"
                                  p:propertyName="creditOption.maxCredits" p:fieldLabel.renderColon="false" p:required="true">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="3" />
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <!-- Multiple -->
                <bean parent="Uif-InputField" p:propertyName="creditOption.credits" p:label="Credits" p:fieldLabel.renderColon="false" p:style="margin-left: 1em;" p:progressiveRender="@{#dp.creditOption.typeKey eq 'kuali.result.values.group.type.multiple'}">
                    <property name="control">
                        <bean parent="Uif-HorizontalCheckboxesControl"/>
                    </property>
                    <property name="optionsFinder">
                        <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.MultipleCreditOptionsKeyValues" p:addBlankOption="false" />
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KS-CourseOfferingEdit-DeliveryAndAssessment" parent="Uif-VerticalBoxSection" p:headerText="Delivery and Assessment">
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="&lt;hr&gt;"/>

                <bean parent="Uif-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField" p:propertyName="coInfo.finalExamType" p:label="Final Exams" p:fieldLabel.renderColon="false" p:labelPlacement="TOP">
                                <property name="control">
                                    <bean parent="Uif-VerticalRadioControl"/>
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.FinalExamOptionsKeyValues" p:addBlankOption="false" />
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <ref bean="KS-CourseOfferingEdit-DeliveryFormats"/>


            </list>
        </property>
    </bean>


    <bean id="KS-CourseOfferingEdit-DeliveryFormats" parent="Uif-HorizontalBoxSubSection" p:refreshWhenChangedPropertyNames="#dp.coInfo.finalExamType">
        <property name="headerText" value="Delivery Formats"/>
        <property name="instructionalText" value="Add the delivery formats to be used for this course offering"/>
        <property name="items">
            <list>
                <bean id="KS-CourseOfferingEdit-FormatOfferingSubSection" parent="Uif-TableCollectionSubSection" p:collectionLookup.render="false">
                    <property name="headerText" value="" />
                    <property name="collectionObjectClass" value="org.kuali.student.enrollment.courseoffering.dto.FormatOfferingInfo" />
                    <property name="propertyName" value="formatOfferingList" />
                    <property name="layoutManager.renderSequenceField" value="false" />
                    <property name="layoutManager.applyAlternatingRowStyles" value="false" />
                    <property name="layoutManager.applyDefaultCellWidths" value="false" />
                    <property name="layoutManager.richTable.render" value="false" />
                    <property name="addLineActions">
                      <list>
                        <bean parent="Uif-SecondaryActionButton-Small" p:methodToCall="addLine" p:actionLabel="add"
                              p:actionEvent="addLine"/>
                      </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField" p:propertyName="formatId"  p:readOnly="@{!#isAddLine}"
                                  p:label="FORMAT" p:labelPlacement="TOP" p:readOnlyDisplayReplacementPropertyName="name" p:required="true">
                                <property name="control">
                                    <bean parent="Uif-DropdownControl"/>
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.FormatOfferingTypeForEditCOTypeKeyValues" p:addBlankOption="false"/>
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:propertyName="gradeRosterLevelTypeKey"
                                  p:label="GRADE ROSTER LEVEL" p:labelPlacement="TOP" p:required="true">
                                <property name="control">
                                    <bean parent="Uif-DropdownControl" />
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.GradeRosterLevelKeyValues"/>
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:propertyName="finalExamLevelTypeKey" p:label="FINAL EXAM DRIVER ACTIVITY"
                                  p:labelPlacement="TOP" p:required="true" p:render="@{#dp.coInfo.finalExamType eq 'STANDARD'}">
                                <property name="control">
                                    <bean parent="Uif-DropdownControl" />
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.FinalExamDriverKeyValues"/>
                                </property>
                            </bean>
                            <bean parent="Uif-MessageField" p:label="FINAL EXAM DRIVER ACTIVITY" p:labelPlacement="TOP" p:fieldLabel.renderColon="false" p:messageText="Alternate exam for this offering" p:render="@{#dp.coInfo.finalExamType eq 'ALTERNATE'}" />
                            <bean parent="Uif-MessageField" p:label="FINAL EXAM DRIVER ACTIVITY" p:labelPlacement="TOP" p:fieldLabel.renderColon="false" p:messageText="No final exam for this offering" p:render="@{#dp.coInfo.finalExamType eq 'NONE'}" />
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KS-CourseOfferingEdit-Waitlist" parent="Uif-VerticalBoxSection" p:headerText="Waitlists">
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="&lt;hr&gt;"/>
                <bean parent="Uif-HorizontalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField-LabelTop" p:propertyName="coInfo.hasWaitlist" p:label="Waitlists" p:fieldLabel.renderColon="false" p:labelPlacement="TOP">
                                <property name="control">
                                    <bean parent="Uif-CheckboxControl">
                                        <property name="checkboxLabel" value="This course offering has a waitlist"/>
                                    </bean>
                                </property>
                            </bean>
                            <bean parent="Uif-HorizontalBoxSection" p:refreshWhenChangedPropertyNames="#dp.coInfo.hasWaitlist">
                                <property name="items">
                                    <list>
                                       <bean parent="Uif-InputField" p:propertyName="coInfo.waitlistLevelTypeKey" p:label="Waitlist Level" p:fieldLabel.renderColon="false" p:labelPlacement="TOP">
                                            <property name="control">
                                                <bean parent="Uif-VerticalRadioControl" p:disabled="@{!#dp.coInfo.hasWaitlist}"/>
                                            </property>
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.WaitlistLevelOptionsKeyValues" />
                                            </property>
                                        </bean>
                                        <bean parent="Uif-InputField" p:label="Waitlist Type" p:propertyName="coInfo.waitlistTypeKey" p:fieldLabel.renderColon="false" p:labelPlacement="TOP">
                                            <property name="control">
                                                <bean parent="Uif-DropdownControl" p:disabled="@{!#dp.coInfo.hasWaitlist}"/>
                                            </property>
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.WaitlistTypeOptionsKeyValues" />
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KS-CourseOfferingEdit-PersonnelSection" parent="Uif-VerticalBoxSection">
        <property name="headerText" value="Personnel"/>
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="&lt;hr&gt;"/>
                <bean id="KS-ActivityOffering-PersonnelSection" parent="Uif-TableCollectionSection"
                      p:layoutManager.numberOfColumns="6"
                      p:layoutManager.style="width: 100%; display:block;">
                    <property name="headerText" value="Affiliated Personnel" />
                    <property name="collectionObjectClass" value="org.kuali.student.enrollment.courseoffering.dto.OfferingInstructorInfo" />
                    <property name="propertyName" value="coInfo.instructors" />
                    <property name="layoutManager.renderSequenceField" value="false" />
                    <property name="layoutManager.separateAddLine" value="false" />
                    <property name="layoutManager.richTable.render" value="false" />
                    <property name="layoutManager.applyDefaultCellWidths" value="false" />
                    <property name="layoutManager.applyAlternatingRowStyles" value="false" />
                    <property name="addLineItems">
                        <list>
                            <bean parent="Uif-InputField" p:propertyName="personId" p:label="ID" >
                                <property name="quickfinder.dataObjectClassName" value="org.kuali.rice.kim.api.identity.Person"/>
                                <property name="quickfinder.fieldConversions">
                                    <map>
                                        <entry key="name" value="personName"/>
                                        <entry key="entityId" value="personId"/>
                                    </map>
                                </property>
                                <property name="quickfinder.viewName" value="ks_simplePersonLookup" />
                            </bean>
                            <bean parent="Uif-InputField" p:propertyName="personName" p:label="Name" p:readOnly="true" />
