<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="KS-RegistrationWindowsManagement-View" parent="KSFormView">
        <property name="id" value="registrationWindowsManagementView"/>
        <property name="title" value="Manage Registration Windows"/>
        <property name="entryPageId" value="selectTermForRegWindows"/>
        <property name="viewHelperServiceClassName" value="org.kuali.student.enrollment.class2.appointment.service.impl.AppointmentViewHelperServiceImpl"/>
        <property name="header">
            <bean parent="Uif-HeaderField-Two"/>
        </property>
        <property name="footer">
            <bean parent="Uif-FooterBase"/>
        </property>
        <property name="items">
            <list>
                <ref bean="KS-RegistrationWindowsManagement-SelectTermPage"/>
                <ref bean="KS-RegistrationWindowsManagement-EditPage"/>

            </list>
        </property>
        <property name="formClass" value="org.kuali.student.enrollment.class2.appointment.form.RegistrationWindowsManagementForm"/>
        <!--<property name="defaultBindingObjectPath" value=""/>-->
        <!--<property name="additionalCssFiles">-->
            <!--<list merge="true">-->
                <!--<value>/ks-enroll/css/acal.css</value>-->
            <!--</list>-->
        <!--</property>-->
    </bean>

</beans>