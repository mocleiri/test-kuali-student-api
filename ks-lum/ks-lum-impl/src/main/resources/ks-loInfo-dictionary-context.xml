 <bean id="loCategoryInfo.expirationDate-parent" abstract="true"		parent="baseKualiExpirationDate">
  <property name="name" value="expirationDate" />
 </bean>
 <bean id="loCategoryInfo.expirationDate" parent="loCategoryInfo.expirationDate-parent" />

 <bean id="loCategoryInfo.id-parent" abstract="true" parent="baseString">
  <property name="name" value="id" />
 </bean>
 <bean id="loCategoryInfo.id" parent="loCategoryInfo.id-parent" />

 <bean id="loCategoryInfo.loRepository-parent" abstract="true"
       parent="baseString">
  <property name="name" value="loRepository" />
 </bean>
 <bean id="loCategoryInfo.loRepository" parent="loCategoryInfo.loRepository-parent" />

 <bean id="loCategoryInfo.name-parent" abstract="true" parent="baseString">
  <property name="name" value="name" />
  <property name="minOccurs" value="1" />
 </bean>
 <bean id="loCategoryInfo.name" parent="loCategoryInfo.name-parent" />

 <bean id="loCategoryInfo.state-parent" abstract="true" parent="baseString">
  <property name="name" value="state" />
 </bean>
 <bean id="loCategoryInfo.state" parent="loCategoryInfo.state-parent" />

 <bean id="loCategoryInfo.type-parent" abstract="true" parent="baseString">
  <property name="name" value="type" />
 </bean>
 <bean id="loCategoryInfo.type" parent="loCategoryInfo.type-parent" />

