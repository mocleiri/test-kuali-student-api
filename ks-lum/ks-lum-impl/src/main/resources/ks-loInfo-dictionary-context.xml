  <property name="name" value="desc" />
  <property name="minOccurs" value="1" />
  <property name="dataObjectStructure" ref="loInfo.desc.RichTextInfo" />
 </bean>
 <bean id="loInfo.desc" parent="loInfo.desc-parent" />

 <bean id="loInfo.desc.RichTextInfo-parent"  abstract="true" parent="org.kuali.student.common.dto.RichTextInfo-parent">
  <property name="name" value="org.kuali.student.lo.dto.RichTextInfo" />
  <property name="attributes">
   <list>
    <ref bean="loInfo.desc.formatted" />
    <ref bean="loInfo.desc.plain" />
