 <bean id="loCategoryInfo.desc.formatted-parent" abstract="true" parent="richTextInfo.formatted-parent">
  <property name="name" value="formatted" />
  <property name="maxLength" value="250" />
 </bean>
 <bean id="loCategoryInfo.desc.formatted" parent="loCategoryInfo.desc.formatted-parent" />

 <bean id="loCategoryInfo.desc.plain-parent" abstract="true" parent="richTextInfo.formatted-parent">
  <property name="name" value="plain" />
  <property name="maxLength" value="250" />
  <property name="minOccurs" value="0" />
 </bean>
