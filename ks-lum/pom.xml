        <module>ks-lum-sql</module>
        <module>ks-lum-api</module>
        <module>ks-lum-impl</module>
        <module>ks-lum-rice</module>
        <module>ks-lum-ui</module>
        <module>ks-lum-web</module>
        <module>ks-lum-program</module>
        <module>ks-lum-ui-common</module>
      </modules>
    </profile>
    <profile>
      <id>sql-only</id>
      <modules>
        <module>ks-lum-sql</module>
      </modules>
    </profile>
    <profile>
      <id>embedded-only-war</id>
      <modules>
        <module>ks-lum-sql</module>
        <module>ks-lum-api</module>
        <module>ks-lum-impl</module>
        <module>ks-lum-rice</module>
        <module>ks-lum-ui</module>
        <module>ks-lum-program</module>
        <module>ks-lum-ui-common</module>
      </modules>
    </profile>
    <profile>
      <id>standalone-only-wars</id>
      <modules>
        <module>ks-lum-sql</module>
        <module>ks-lum-api</module>
        <module>ks-lum-impl</module>
        <module>ks-lum-rice</module>
        <module>ks-lum-ui</module>
        <module>ks-lum-program</module>
        <module>ks-lum-ui-common</module>
      </modules>
    </profile>
    <profile>
      <id>skip-lum-war</id>
      <modules>
        <module>ks-lum-sql</module>
        <module>ks-lum-api</module>
        <module>ks-lum-impl</module>
        <module>ks-lum-rice</module>
        <module>ks-lum-ui</module>
        <module>ks-lum-program</module>
        <module>ks-lum-ui-common</module>
      </modules>
    </profile>
    <profile>
      <id>skip-developer-wars</id>
      <modules>
        <module>ks-lum-sql</module>
        <module>ks-lum-api</module>
        <module>ks-lum-impl</module>
