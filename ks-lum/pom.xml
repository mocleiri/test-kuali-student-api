      </modules>
    </profile>
    <profile>
      <id>skip-all-wars</id>
      <modules>
        <module>ks-lum-sql</module>
        <module>ks-lum-api</module>
        <module>ks-lum-impl</module>
        <module>ks-lum-rice</module>
        <module>ks-lum-ui</module>
        <module>ks-lum-program</module>
        <module>ks-lum-ui-common</module>
      </modules>
    </profile>
    <profile>
      <id>skip-ui</id>
      <modules>
        <module>ks-lum-sql</module>
        <module>ks-lum-api</module>
        <module>ks-lum-impl</module>
        <module>ks-lum-rice</module>
      </modules>
    </profile>
  </profiles>
  <repositories>
    <repository>
      <id>kuali.nexus</id>
      <name>Nexus Repository Manager</name>
      <url>http://nexus.kuali.org/content/groups/public</url>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
    </repository>
  </repositories>
</project>